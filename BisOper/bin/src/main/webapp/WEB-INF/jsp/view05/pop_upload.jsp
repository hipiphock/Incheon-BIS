<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<title>시나리오 업로더</title>
</head>
<body>
	<!-- <div class="pop_wrap" id="pop_Upload"> -->
        <div class="si_pop_wrap pop" id="pop_Upload">
        	<div id="loading_wrap"><img alt='로딩중' src="./images/loader.gif"></div>
            <div class="pop_title">
                <h2>MediaFile Uploader</h2>
                <!-- <p><a class="pop_close" href="javascript:void(0);"><img src="./images/close_white.png" alt="닫기"></a></p> -->
            </div>
            <div class="pop_conbody si_pop"> 
                     <!--위 작은버튼-->
                    <div class="pop_left_btn">
                        <ul>
                            <li id="file_add"><a href="javascript:void(0);" class="s_btn bit_pop2_btn"><img src="./images/icon_plus.png">파일추가</a></li>
                            <li id="file_delete"><a href="javascript:void(0);" class="s_btn bit_pop2_btn">선택삭제</a></li>
                        </ul>
                    </div> 
                    <!--위 작은버튼끝--> 
                <div id="files" class="business_pop_con pop_new">
                    <div class="main_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                    	<table id="file_list"></table>
                    </div>
                </div>
                    <!--아래 버튼 시작-->
                    <div class="pop_bottom_btn2 pop_new_btn">
                        <ul>
                            <li id="file_upload_start" class="bottom_save"><a href="javascript:void(0);"><img src="./images/icon_ok.png">업로드 시작</a></li>
                            <li id="file_close" class="bottom_close pop_close"><a href="javascript:void(0);"><img src="./images/close_black.png">닫기</a></li>
                        </ul>
                    </div>
                    <!--아래 버튼 끝-->  
           </div>
        </div> 
        <div class="pop_back"></div>
	<!-- </div> -->
</body>
</html>
