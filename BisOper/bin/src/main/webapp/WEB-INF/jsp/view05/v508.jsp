<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<head>
<head>
<meta charset="UTF-8">
  <!--<meta name="viewport" content="width=device-width, initial-scale=1">-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
<title>BIT 그룹</title>
<link rel="stylesheet" type="text/css" href="./css/sub_bootstrap.css"/>
<link rel="stylesheet" type="text/css" href="./css/style_comm.css"/>
<link rel="stylesheet" type="text/css" href="./css/style_sub.css"/>

<script src="./js/comm/commGrid.js"></script>
<script src="./js/comm/commDialog.js"></script>
<script src="./js/comm/common.js"></script>

<script src="./js/v05/v508.js"></script>

</head>

<body>
	<jsp:include page="pop_group_add.jsp"/>
	<div class="" id="pop_bit_grp">
        <div class="poplus_wrap pop">
            <div class="pop_title2">
                <h2>BIT 그룹 관리</h2>
            </div>
            <div id="container" class="pop_conbody2 bitplus_pop"> 
                <!--왼쪽시작-->
                <div class="bit_group_left">
                    <!--왼쪽위 작은버튼-->
                    <div class="pop_left_btn2">
                        <ul>
                            <li><a id="pop_group_add" href="javascript:void(0);" class="s_btn bit_pop2_btn"><img src="./images/icon_plus.png">추가</a></li>
                            <li><a id="pop_group_mod" href="javascript:void(0);" class="s_btn bit_pop2_btn">수정</a></li>
                            <li><a id="pop_group_del" href="javascript:void(0);" class="s_btn bit_pop2_btn">삭제</a></li>
                        </ul>
                    </div>
                    <!--왼쪽위작은버튼끝-->
                    <div class="bit_popcon1">
                        <div class="main_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                        	<table id="pop_group_list"></table>
                        </div>
                    </div>
                    <div class="bit_popcon2">
                        <div class="layer_title">
                            <h3>기본사항</h3>
                        </div>
                        <div class="main_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                        	<table id="pop_group_detail_list"></table>
                        </div>
                    </div>    
                </div>   
                <!--왼쪽끝-->
                <div class="bit_arrow_btn"><!--이동버튼--> 
                        <button id="btn_left" class="left_b"></button><!--활성상태--> 
                        <button id="btn_right" class="right_b"></button><!--비활성상태: 클래스명에 '_b'를 추가해주세요--> 
                </div>
                <!--오른쪽시작-->
                <div class="bit_group_right">
                    <!--오른쪽 위 검색 시작-->
                    <div class="bit_top2_serch">
                        <div class="bit_pop_top2"> 
                                <input id="pop_group_input_search" type="text" class="subtop_in_text" name="ID" placeholder="검색어 입력">
                                <span><img id="pop_group_img_search" class="sub_serch_btn" src="./images/serch_gray.png" alt="검색버튼"></span>
                        </div>
                    </div>
                    <!--오른쪽 위 검색 끝-->
                    <div class="bit_popcon3">
                        <div class="main_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                        	<table id="pop_group_bit_list"></table>
                        </div>
                    </div>
                </div>
                <!--오른쪽끝-->
                <!--아래 버튼 시작-->
                <div class="pop_bottom_btn2">
                    <ul>
                        <li id="pop_group_save" class="bottom_save"><a href="javascript:void(0);"><img src="./images/icon_ok.png">저장</a></li>
                        <li id="pop_group_cancel" class="bottom_close"><a href="javascript:void(0);"><img src="./images/close_black.png">취소</a></li>
                    </ul>
                </div>
                <!--아래 버튼 끝-->
           </div>
        </div> 
        <div class="pop_back"></div>
	</div>
</body>
</html>
