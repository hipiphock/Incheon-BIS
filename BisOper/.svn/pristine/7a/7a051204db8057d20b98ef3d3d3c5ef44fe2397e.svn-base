/*top검색*/
.top_serch{position: absolute; top: 14px; right:15px; background: #1f77d1; border: 1px solid #206dbc;  width: 180px; padding:7px 2px;}
.top_serch img{vertical-align: middle; cursor: pointer;}
.top_serch_textbox{border: none; background:#1f77d1; color: #fff; padding-left: 5px; width: 145px; }

/*top메뉴*/
.login_top h1{padding: 18px;}
.login_top, .top_menu{width: 100%; height: 63px; background-color: #4594e5; border-bottom: 1px solid #1963af;} /*<----------메뉴배경*/
.menu_logo{float: left; padding: 13px;}/*<----------로고*/
h1 a img{vertical-align: middle; margin-right: 10px;}
h1 span{ font-size: 14px; font-weight: normal; border-left: 1px solid #a2caf2; vertical-align: middle; color: #fff; padding-left: 10px;}
.box_set {position: absolute; right: 0; top: 0; width:63px; height:48px; border-left:1px solid #1963af; font-size: 12px; cursor:pointer; text-align: center;  padding: 8px 0;}  /*<----------설정*/
.manu_text{display: block; height: 12px; color: #fff; font-size: 12px;}

.manu_center{position: absolute; width:1070px; left: 50%; margin-left: -495px; top: 0;}
