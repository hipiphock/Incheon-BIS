<%@ page language="java" contentType="text/html; charset=UTF-8"
    pageEncoding="UTF-8"%>
<!DOCTYPE html>
<html>
<meta charset="UTF-8">
  <!--<meta name="viewport" content="width=device-width, initial-scale=1">-->
    <meta name="viewport" content="width=device-width, initial-scale=1.0, minimum-scale=1.0, maximum-scale=1.0, user-scalable=no" />
	<meta http-equiv="X-UA-Compatible" content="IE=edge, chrome=1">
<title>BIT 스케줄 작성</title>
<link rel="stylesheet" type="text/css" href="./css/sub_bootstrap.css"/>
<link rel="stylesheet" type="text/css" href="./css/style_comm.css"/>
<link rel="stylesheet" type="text/css" href="./css/style_sub.css"/>

<script src="./js/comm/commGrid.js"></script>
<script src="./js/comm/commDialog.js"></script>
<script src="./js/comm/common.js"></script>

<script src="./js/v05/v520.js"></script>
<body>
	<%-- <jsp:include page="pop_upload.jsp" />
	<jsp:include page="pop_text.jsp" /> --%>
	<div class="" id="pop_mng_scene">
        <div class="schedule_wrap pop">
            <div class="pop_title2">
                <h2>스케줄 작성(인천/확대)</h2>
            </div>
            <div id="container" class="pop_conbody2 schedule_pop"> 
                <!--왼쪽시작-->
               
                <!--왼쪽끝-->
                <!--가운데시작-->
                <div class="schedule_left">
                    <div class="layer_title">
                        <h3>스케줄 목록</h3>
                    </div>
                    <div class="btn_box6">
                    	<ul>
                            <li><a id="btn_del" class="s_btn sina_btn btnimg2" href="javascript:void(0);">선택삭제</a></li>
						</ul>
                    </div>
                    <div class="sina_ch">
                        <div class="main_chart pop_middle_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                        <table id="schedule_list"></table>
                        </div>
                    </div>
                     <!-- 상세항목 버튼 -->
                    <div class="subcon2_right_btn">
                    	<ul class="con2_r">
                    		<li><a id="btn_save" href="javascript:void(0);"><img src="./images/icon_ok.png">저장</a></li>
                    		<li><a id="btn_cancel" href="javascript:void(0);"><img src="./images/close_black.png">취소</a></li>
                    	</ul>
                    </div>
                </div> 
                <!--가운데끝-->
                <!--오른쪽시작-->
                <div class="schedule_right">
           	    	<div class="layer_title">
		                <h3>이미지 자료</h3>
		            </div>
                    <!--탭컨텐츠시작-->                      
				    <div class="tab-content_percent">
                        <!--이미지자료 시작-->
                        <div class="tab-pane active" id="si_tab1">
                        	
                            <div class="sch_pop3_two">
                                <div class="btn_box99">
                                    <ul>
                                        <li><a class="s_btn sina_btn add" id="btn_add_img" href="javascript:void(0);"><img style="margin-top: -3px;" src="./images/icon_plus.png">목록추가</a></li>
                                    </ul>
                                </div>
                                <table class="chart99"  border="0" cellspacing="0" cellpadding="0">
                                <tr >
                                    <th style="border-top: 1px solid #eaeaea;">파일명</th>
                                    <td style="border-top: 1px solid #eaeaea;">
                                    	<input type="text" class="in2 in_text base_text0" id="input_filename" readonly="readonly">
                                    </td>
                                </tr>
                                <tr>
                                    <th>다운로드시작시간</th>
                                    <td>
                                    	<input type="text" class="in in_text_b sch_in1" id="input_down_date">
                                        <input type="text" class="in in_text_b sch_in1" id="input_down_time">
                                    </td>
                                </tr>
                                <tr>
                                    <th>RUN TIME</th>
                                    <td>
                                        <input type="text" class="in in_text_b sch_in1" id="input_run_time">
                                    </td>
                                </tr>
                                <tr>
                                    <th>표출시작시간</th>
                                    <td>
                                    	<input type="text" class="in in_text_b sch_in1" id="input_disp_time">
                                    </td>
                                </tr>
                                </table>
                            </div>
                            <div class="schedule_ch2">
                                <div class="main_chart pop_right_chart"><!--<!--.main_chart 사이즈 [ width: 100%; height:100%;]-->
                                	<table id="pop_image_list"></table>
                                </div>
                            </div>
                        </div>
                    </div>
                </div> 
                <!--오른쪽끝-->

              
           </div>
        </div> 
        <div class="pop_back"></div>
	</div>
</body>
</html>
